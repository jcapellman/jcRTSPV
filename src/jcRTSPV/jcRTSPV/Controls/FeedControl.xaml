<UserControl
    x:Class="jcRTSPV.Controls.FeedControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:jcRTSPV.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid>
        <MediaElement x:Name="meFeed" AutoPlay="True" IsFullWindow="False" IsMuted="True" />

        <Button HorizontalAlignment="Right" VerticalAlignment="Top" Tapped="symbolMute_OnTapped" Margin="0,20,0,0">
            <TextBlock x:Name="tbIcon" FontFamily="Segoe MDL2 Assets" Foreground="White" Text="&#xE198;" FontSize="60" />
        </Button>
    </Grid>
</UserControl>
